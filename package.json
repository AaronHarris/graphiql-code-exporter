{
  "name": "graphiql-code-exporter",
  "version": "1.0.5",
  "description": "Export working code snippets from GraphiQL queries",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "build:es": "babel -d es src --ignore __tests__",
    "build:lib": "cross-env BABEL_ENV=commonjs babel -d lib src --ignore __tests__",
    "build": "yarn build:es && yarn build:lib",
    "release": "yarn build && npm publish",
    "test": "cross-env BABEL_ENV=commonjs jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OneGraph/graphiql-code-exporter.git"
  },
  "keywords": [
    "onegraph",
    "graphql",
    "graphiql",
    "tools",
    "devTools",
    "DX"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OneGraph/graphiql-code-exporter/issues"
  },
  "homepage": "https://github.com/OneGraph/graphiql-code-exporter#readme",
  "peerDependencies": {
    "graphql": "^14.1.1",
    "prettier": "^1.16.0",
    "react": "^15.6.0 || ^16.0.0"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.0.8",
    "prismjs": "^1.15.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.0",
    "babel-core": "^6.6.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^5.2.0",
    "jest": "^19.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.2.0"
  }
}
